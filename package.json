{
  "name": "myblogapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www ",
    "dev": "NODE_ENV=development npm-run-all server",
    "prod": "npm-run-all clean build server:prod",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "watch": "nodemon",
    "sync:database:development": "NODE_ENV=development babel-node ./src/sequelize/sync",
    "sync:database:production": "NODE_ENV=production babel-node ./src/sequelize/sync"
  },
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "aws-sdk": "^2.814.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-register": "^6.26.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.5.1",
    "module-resolver": "^1.0.0",
    "morgan": "~1.9.0",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.26.1",
    "rimraf": "^3.0.0",
    "sequelize": "^5.21.3",
    "unsplash-js": "^6.0.0"
